<style>
  /* for level setting popup title bar*/
  .displaylist_settings
  {
    background: #d3d3d3 none repeat scroll 0 0;
    color: #000000;
    display: none;
    border: 1px solid #504f4f;
    margin: 12px 0 0 1%;
    width: 212px;
    position: relative;
    z-index: 11;
  }

  .pop-dropdown
  {
    list-style:none;
    border-top: 4px solid #504f4f;
    margin:0;
    padding:0;
  }
  .pop-dropdown > li > a
  {
    text-decoration:none;
    clear: both;
    color: #333;
    display: block;
    font-weight: 400;
    padding: 3px 14px;
    white-space: nowrap;
    text-align: left;
  }
  .pop-dropdown > li > a:focus,.pop-dropdown > li > a:hover
  {
    background:#BFBFBF;
    color:#333;
  }

  .modal-body{min-height:486px;}

  .pop-pointer-dd {
    margin-left: 31px;
    position: absolute;
    top: -11px;
  }

</style>



<script type="text/javascript">
