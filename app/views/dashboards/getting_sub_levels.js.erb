var f = '<%= params[:id] %>';
var order = $("#sub_level_"+f).attr('order');
if(order == "not-open") {
    $("<%= escape_javascript render(:partial => 'sub_levels',:locals => { :sub_level => @sub_levels ,:level_wgus => @level_wgus }) %>").insertAfter("#sub_level_" + f);
    $("#sub_level_" + f).attr('order', 'open');
}